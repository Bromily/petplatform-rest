<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.petplatform.mapper.MemberMapper">

    <select id="selectMember" resultType="com.petplatform.dto.MemberDto" parameterType="String">
        select * from user where userid=#{userid}
    </select>

    <insert id="insertMember" parameterType="com.petplatform.dto.MemberDto">
        insert user values (#{userid}, #{password}, #{name}, #{birth}, #{address}, #{phone_number}, #{user_type})
    </insert>
</mapper>